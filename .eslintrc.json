{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalRestObjectSpread": true
    }
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": 1,
    "no-param-reassign": 1,
    "no-underscore-dangle": 1
  }
}
